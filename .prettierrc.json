{
  "arrowParens": "always",
  "bracketSameLine": true,
  "bracketSpacing": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "singleAttributePerLine": false,
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "proseWrap": "always",
  "insertPragma": false,
  "printWidth": 80,
  "requirePragma": true,
  "tabWidth": 3,
  "useTabs": true,
  "embeddedLanguageFormatting": "auto",
  "max_line_length": 80
}